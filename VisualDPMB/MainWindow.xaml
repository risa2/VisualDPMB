<Window x:Class="VisualDPMB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualDPMB"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="600" Background="White">
    <Grid>
        <Canvas Name="image_platno" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Image Name="image" Canvas.Left="0" Canvas.Top="0"/>
        </Canvas>
        <Canvas Name="platno" HorizontalAlignment="Left" VerticalAlignment="Top" />
    </Grid>
</Window>
